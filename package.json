{
  "name": "@google-cloud/trace-agent",
  "version": "2.1.3",
  "description": "Node.js Support for StackDriver Trace",
  "main": "build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoogleCloudPlatform/cloud-trace-nodejs.git"
  },
  "scripts": {
    "test": "node ./build/scripts npm-compile init-test-fixtures run-unit-tests",
    "non-interference": "node ./build/scripts npm-compile test-non-interference",
    "system-test": "node ./build/scripts npm-compile decrypt-service-account-key run-system-tests",
    "changelog": "./bin/run-changelog.sh",
    "coverage": "node ./build/scripts npm-compile init-test-fixtures run-unit-tests-with-coverage report-coverage",
    "bump": "./bin/run-bump.sh",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "postinstall": "tsc -p scripts",
    "prepare": "npm run compile"
  },
  "keywords": [
    "google",
    "tracing",
    "profiling"
  ],
  "author": {
    "name": "Google Inc."
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=0.12"
  },
  "devDependencies": {
    "@google-cloud/datastore": "^1.0.2",
    "@types/glob": "^5.0.32",
    "@types/mocha": "^2.2.42",
    "@types/ncp": "^2.0.1",
    "@types/node": "^8.0.26",
    "@types/once": "^1.4.0",
    "@types/pify": "0.0.28",
    "changelog-maker": "^2.2.2",
    "coveralls": "^2.11.8",
    "express": "^4.15.2",
    "glob": "^7.0.3",
    "google-auto-auth": "^0.7.0",
    "got": "^5.7.1",
    "gts": "latest",
    "istanbul": "^0.4.2",
    "jshint": "^2.9.1",
    "mocha": "^3.0.0",
    "ncp": "^2.0.0",
    "nock": "^9.0.0",
    "once": "^1.4.0",
    "pify": "^3.0.0",
    "proxyquire": "^1.4.0",
    "request": "^2.81.0",
    "source-map-support": "^0.4.16",
    "standard-version": "^4.2.0",
    "timekeeper": "^1.0.0",
    "tmp": "0.0.31",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "@google-cloud/common": "^0.13.2",
    "continuation-local-storage": "^3.1.4",
    "extend": "^3.0.0",
    "gcp-metadata": "^0.2.0",
    "is": "^3.2.0",
    "lodash.findindex": "^4.4.0",
    "lodash.isequal": "^4.0.0",
    "lodash.merge": "^4.6.0",
    "methods": "^1.1.1",
    "semver": "^5.0.1",
    "shimmer": "^1.0.0",
    "uuid": "^3.0.1"
  }
}
